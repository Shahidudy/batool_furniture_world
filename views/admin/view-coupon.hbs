<section>
        <div id="right-panel" class="right-panel">
        <div class="">
            <a href="/admin/add-coupon" class="btn btn-success mt-1 float-right">Add Coupon</a>
        </div>
        <table class="table" id="coupon">
            <thead>
                <tr>
                    <th></th>
                    <th>No</th>
                    <th scope="col">Coupon</th>
                    <th scope="col">Discount Amount</th>
                    <th scope="col">Expiry Date</th>                  
                    <th scope="col">Option</th>                  
                </tr>
            </thead>
            <tbody>
                {{#each coupondetails}}
                <tr>
                    <td></td>
                    <td>{{inc @index}} </td>
                    <td>{{this.coupon}} </td>
                     <td>{{this.discount}} </td>
                      <td>{{this.date}} </td>
                        <td>
                    <a href="/admin/delete-coupon/{{this._id}}" class="btn btn-danger" onclick="return confirm('Are  you want to delete {{this.Name}} ?')">Delete</a>
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
        </div>
</section>
